{
  "name": "@acme/core",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "author": "Sidwebworks",
  "description": "A CLI Framework",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
